# =============================================================================
# TUNNEL PROXY CONFIGURATION
# =============================================================================

PORT=9000

# Redis connection string
REDIS_URL=redis://localhost:6379

# Unique instance ID (for distributed deployments)
INSTANCE_ID=local-dev

# JWT secret for tunnel authentication (must match server's TUNNEL_JWT_SECRET)
TUNNEL_JWT_SECRET=

# Internal API key (must match server's INTERNAL_API_KEY)
INTERNAL_API_KEY=dev-internal-key

# Server URL for API calls
SERVER_URL=http://localhost:8888/api
